{
	"name": "todo-pwa",
	"version": "0.0.1",
	"author": "barbajoe",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"all": "pnpm clean && pnpm install && pnpm check && pnpm build",
		"clean": "rm -rf .netlify .svelte-kit build",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "3.3.1",
		"@sveltejs/kit": "2.7.4",
		"@sveltejs/vite-plugin-svelte": "4.0.0",
		"svelte": "5.1.9",
		"svelte-check": "4.0.5",
		"tslib": "2.8.1",
		"typescript": "5.6.3",
		"vite": "5.4.10",
		"vite-plugin-pwa": "0.20.5"
	}
}
